    "react-bootstrap-pagination": "^1.0.4",
    "react-dom": "^15.6.1",
    "react-fontawesome": "^1.6.1",
    "react-paginate": "^5.0.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "sass-loader": "^6.0.6",
    "sequelize": "^4.7.5",
    "sequelize-cli": "^2.8.0",
    "style-loader": "^0.18.2",

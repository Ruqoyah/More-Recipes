    "eslint-config-react": "^1.1.7",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-react": "^7.5.1",
    "eslint-to-editorconfig": "^1.2.0",
    "fetch-mock": "^5.13.1",
    "jest": "^22.0.1",
    "moxios": "^0.4.0",
    "nightwatch": "^0.9.19",
    "react-addons-test-utils": "^15.6.2",
    "react-test-renderer": "^15.6.1",
    "redux-mock-store": "^1.3.0",
    "sinon": "^4.1.3"
  }
}

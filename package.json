{
  "name": "more-recipes",
  "version": "1.0.0",
  "description": "More-Recipes provides a platform for users to share the awesome and exciting recipe ideas they have invented or learnt. Suppose a user comes up with a food recipe, he/she can post it on More-Recipes and get feedback in form of reviews and votes from other users who explore that recipe. Users can also keep a list of their favorite recipes on the application.",
  "main": "app.js",
  "scripts": {
    "start": "babel-node app.js",
    "server:test": "cross-env NODE_ENV=test nyc --reporter=html --reporter=text mocha --timeout 60000 --compilers js:babel-register server/test",
    "coverage": "NODE_ENV=test nyc report --reporter=text-lcov | coveralls",
    "lint": "eslint ./",
    "build": "webpack --progress --config ./webpack.config.prod.js",
    "heroku-postbuild": "NODE_ENV=production &&npm run build",
    "client:test": "jest --coverage",
    "test": "npm run client:test && npm run server:test",
    "pree2e:test": "sequelize db:migrate:undo:all --env=localTest && sequelize db:migrate --env=localTest",
    "e2e:test": "nightwatch ./e2e-tests/*.test.js",
    "start-server:e2e": "NODE_ENV=localTest babel-node app.js"
  },
  "engines": {
    "node": "8.1.x",
    "npm": "5.0.x"
  },
  "nyc": {
    "exclude": [
      "server/test/**.js"
    ]
  },
  "dependencies": {
    "axios": "^0.16.2",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "6.8.0",
    "bcrypt": "^1.0.2",
    "body-parser": "^1.17.2",
    "bootstrap": "^4.0.0-beta",
    "chai": "^4.1.1",
    "cheerio": "^1.0.0-rc.2",
    "clean-webpack-plugin": "^0.1.17",
    "cloudinary-react": "^1.0.4",
    "colors": "^1.1.2",
    "compression": "^1.7.1",
    "cors": "^2.8.5",
    "coveralls": "^2.13.1",
    "cross-env": "^5.1.1",
    "css-loader": "^0.28.7",
    "dotenv": "^4.0.0",
    "dotenv-webpack": "^1.5.4",
    "eventsource-polyfill": "0.9.6",
    "expect": "^1.20.2",
    "express": "^4.15.4",
    "express-validator": "^4.1.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^0.11.2",
    "font-awesome": "^4.7.0",
    "history": "^4.7.2",
    "html-webpack-plugin": "^2.30.1",
    "image-webpack-loader": "^3.4.2",
    "imports-loader": "^0.7.1",
    "intl-messageformat": "^1.3.0",
    "jquery": "^3.2.1",
    "jquery-toast-plugin": "^1.3.1",
    "jsdom": "8.5.0",
    "jsonwebtoken": "^7.4.3",
    "lodash": "^4.17.4",
    "mocha": "^3.5.0",
    "mocha-lcov-reporter": "^1.3.0",
    "moment": "^2.19.2",
    "node-sass": "^4.5.3",
    "nodemailer": "^4.1.0",
    "nodemon": "^1.12.1",
    "nyc": "^11.2.0",
    "open": "0.0.5",
    "path": "^0.12.7",
    "pg": "^7.2.0",
    "pg-hstore": "^2.3.2",
    "prop-types": "^15.6.0",
    "react": "^15.6.1",

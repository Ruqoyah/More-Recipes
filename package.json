    "winston": "^2.3.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ruqoyah/More-Recipes.git"
  },
  "keywords": [
    "More",
    "Recipes",
    "Application"
  ],
  "author": "Rukayat Odukoya",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Ruqoyah/More-Recipes/issues"
  },
  "jest": {
    "testMatch": [
      "<rootDir>/client/test/**/*.test.js?(x)"
    ],
    "verbose": true,
    "collectCoverage": true
  },
  "homepage": "https://github.com/Ruqoyah/More-Recipes#readme",
  "devDependencies": {
    "babel-eslint": "^8.0.3",
    "babel-jest": "^22.0.3",
    "chai-jest-snapshot": "^2.0.0",
